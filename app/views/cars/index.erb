<style>
 body {text-align:center;}
</style>
<body>
  <h1>Cars </h1>
  <h2> Welcome:
    <%=current_user.name.upcase %>
    </h2>
  <%@cars.each do |car|%>
  <li> <a href="/cars/<%=car.id%>"><%= car.name %></a></li>
  <% end %>
  <br/>

  <div>
    <% if logged_in? %>
    <a href="/cars/new">Add a new car</a>
    <% end %>
  </div>
  <a href="/logout">Log out</a>
</body>
